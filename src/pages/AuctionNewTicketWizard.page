                       <!-- *********** C&P Code End *************** -->
                    </apex:outputpanel> 
                </apex:pageblockbuttons> 
                  
                <apex:pageMessage summary="{!strSaveResults}" severity="info" strength="2" rendered="{!strSaveResults!=null}" />  
